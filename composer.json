{
    "name": "honvid/yshop",
    "description": "One shop project by Yii 2 ",
    "keywords": ["yii2", "framework", "advanced", "shop"],
    "homepage": "http://www.honvid.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/Honvid/yshop/issues?state=open",
        "wiki": "https://github.com/Honvid/yshop/wiki",
        "source": "https://github.com/Honvid/yshop"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "dmstr/yii2-adminlte-asset": "2.*",
        "mdmsoft/yii2-admin": "~2.0",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-widgets": "*",
        "mongosoft/yii2-upload-behavior": "*",
        "kartik-v/yii2-editable": "^1.7",
        "kartik-v/yii2-mpdf": "^1.0",
        "kartik-v/krajee-markdown-editor": "dev-master",
        "kartik-v/yii2-builder": "1.5.0.x-dev",
        "yzalis/identicon": "1.1.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
composer.json
